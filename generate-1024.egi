#!/usr/bin/env egison
(load-file "./prime-mr.egi")
(define $main
  (lambda [$args] (each display (take 1 (filter prime-mr? (filter odd? rand-nats-1024bits))))))

; 1024bit first number 10...0
(define $NUM-FIRST )

; 1024bit last number 11...1
(define $NUM-LAST )

; 1024bitの整数の無限配列
(define $rand-nats-1024bits
  (letrec {[$rand-nats-impl
         (lambda [$fn]
           {(fn NUM-FIRST NUM-LAST) @(rand-nats-impl fn)})]} (rand-nats-impl pure-rand)))
